//fais par nous-même

test347.txt
	3+4*5

test347.out
	23

//automatiquement généré

test347.moncode
	.start
		push	3
		push	4
		push	5
		mul
		add
		dbg
		halt

test347.resultat
	23

//script

for fichier = *.txt dans repetoire des test
	compilateur fichier.txt > fichier.moncode
	msm fichier.moncode > fichier.resultat
	if fichier.resultat different de fichier.out
		Gueuler très fort


